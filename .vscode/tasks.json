{
    "tasks": [
        {
            "type": "shell",
            "label": "Resource: rc.exe build app.rc",
            "command": "rc",
            "args": [
                "/nologo",
                "${workspaceFolder}\\app.rc"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [],
            "group": "none"
        },
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build main.cpp",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/std:c++17",
                "/nologo",
                "/Fe${workspaceFolder}\\main.exe",
                "${workspaceFolder}\\main.cpp",
                "${workspaceFolder}\\fungsi\\logika.cpp",
                "${workspaceFolder}\\fungsi\\display\\display.cpp",
                "${workspaceFolder}\\fungsi\\history\\history.cpp",
                "${workspaceFolder}\\fungsi\\tombol\\keyboard.cpp",
                "${workspaceFolder}\\fungsi\\tombol\\tombol.cpp",
                "${workspaceFolder}\\app.res",
                "user32.lib",
                "gdi32.lib",
                "comctl32.lib",
                "/link",
                "/SUBSYSTEM:WINDOWS"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "dependsOn": [
                "Resource: rc.exe build app.rc"
            ],
            "dependsOrder": "sequence",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "type": "shell",
            "label": "Release: build package (cl.exe)",
            "command": "cmd.exe",
            "args": [
                "/d",
                "/c",
                "\"${workspaceFolder}\\scripts\\build_release.bat\""
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": "none"
        }
    ],
    "version": "2.0.0"
}
